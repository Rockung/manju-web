{
  "name": "manju-web",
  "description": "A tool for building website directly from markdown docs",
  "version": "0.2.1",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --content-base public",
    "build": "cross-env NODE_ENV=production webpack",
    "pack": "npm run build && node .\\scripts\\pack.js",
    "lint": "eslint . --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://gitee.com/mr-abc/manju-web.git"
  },
  "keywords": [
    "website",
    "markdown",
    "marked",
    "svelte",
    "mathjax",
    "mermaid",
    "vega",
    "ppt",
    "slide"
  ],
  "author": "Rockung(rockung@lesiji.top)",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-arrow-functions": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "archiver": "^4.0.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "eslint": "^7.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "readdirp": "^3.4.0",
    "style-loader": "^1.2.1",
    "svelte": "^3.22.2",
    "svelte-loader": "^2.13.6",
    "terser-webpack-plugin": "^3.0.1",
